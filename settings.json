{
  "files.autoSave": "afterDelay",
  "editor.tabSize": 2,
  "window.zoomLevel": 0,
  "latex-workshop.view.pdf.viewer": "browser",
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "editor.wordWrap": "on",
  "python.jediEnabled": false,
  "npm.enableScriptExplorer": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font','Operator Mono Lig','ubuntu Mono','consolas','fira code', monospace",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.scrollback": 1e27,
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.startupEditor": "welcomePage",
  "scm.alwaysShowActions": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "terminal.integrated.shell.linux": "/bin/zsh",
  "dart.flutterSdkPath": "/home/supriyo/snap/flutter/common/flutter",
  "editor.rulers": [80, 98, 120],
  "terminal.integrated.rendererType": "dom",
  "editor.fontLigatures": true,
  // "editor.fontLigatures": "'ss01','ss03', 'ss06'",
  "gitlens.currentLine.scrollable": false,
  "files.associations": {
    "*.html.eex": "html",
    "*.erb": "erb",
    "*.html.erb": "html"
  },
  "emmet.includeLanguages": {
    "HTML (Eex)": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "ionic-preview.host": "localhost",
  "ionic-preview.port": 8100,
  "editor.minimap.maxColumn": 80,

  // font settings for firaflott

  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic (=FlottFlott)
          "comment",
          "entity.name.type.class", //class names
          "keyword", //import, export, return…
          "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.class.js" //class keyword
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics (VSCode has some defaults for italics)
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },

  // settings for material gecko

  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.fontWeight": "400",

  "bracket-pair-colorizer-2.colors": ["#D39E17", "#A15DEF", "#3398DB"],

  /////////////////////////////////////////////////////////////////////////////////
  // "editor.cursorStyle": "line",
