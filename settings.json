{
    "files.autoSave": "afterDelay",
    "editor.tabSize": 2,
    "window.zoomLevel": -1,
    "latex-workshop.view.pdf.viewer": "browser",
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "editor.wordWrap": "on",
    "python.jediEnabled": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
